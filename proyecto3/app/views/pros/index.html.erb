<p id="notice"><%= notice %></p>

<h1>Listing Pros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre proy</th>
      <th>Horas</th>
      <th>Alumno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pros.each do |pro| %>
      <tr>
        <td><%= pro.nombre_proy %></td>
        <td><%= pro.horas %></td>
        <td><%= pro.alumno %></td>
        <td><%= link_to 'Show', pro %></td>
        <td><%= link_to 'Edit', edit_pro_path(pro) %></td>
        <td><%= link_to 'Destroy', pro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pro', new_pro_path %>
